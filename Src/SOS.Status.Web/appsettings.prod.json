{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "BlazorRenderMode": "Server", // "WebAssembly", "Server" or "Auto"
  "HttpClients": {
    "SosObservationsApi": {
      "BaseAddress": "https://sos-search.artdata.slu.se/"
    },
    "SosAdministrationApi": {
      "BaseAddress": "https://sos-hangfire.artdata.slu.se/"
