@using Microsoft.AspNetCore.Components.WebAssembly.Hosting
@inject IAppEnvironment AppEnvironment

<MudNavMenu>
    <!-- Status -->    
    <MudNavLink Href="" Match="NavLinkMatch.All" Icon="@Icons.Material.Outlined.CheckCircle">Status</MudNavLink>    

    <!-- Observations search -->
    <MudNavLink Href="observationssearch" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Outlined.Search">Observations search</MudNavLink>

    <!-- Taxon diagram -->    
    <MudNavLink Href="taxondiagram" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Outlined.PieChartOutline">Taxon diagram</MudNavLink>

    <!-- Claims (Auth) -->    
    <MudNavLink Href="claims" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Outlined.Lock">Claims (Auth)</MudNavLink>

    @if (AppEnvironment.IsLocalOrDev())
    {
        <!-- Dev -->
        <MudNavGroup Title="Dev" Expanded="false" Icon="@Icons.Material.Outlined.Build">
            <!-- Blazor samples -->
            <MudNavLink Href="blazorsamples?id=5" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Outlined.Code">Blazor samples</MudNavLink>      
            <MudNavLink Href="counter" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Outlined.Numbers">Persistent state</MudNavLink>
        </MudNavGroup>
    }
</MudNavMenu>

